run = "sh ./build.sh && cd ./deploy && ./worldflag-discord-bot"

hidden = ["bin", "obj"]

[env]
DOTNET_NOLOGO = "1"
DOTNET_CLI_TELEMETRY_OPTOUT = "1"
DOTNET_SYSTEM_GLOBALIZATION_INVARIANT = "1"

[packager]
language = "dotnet"

[packager.features]
packageSearch = true

[gitHubImport]
requiredFiles = [".replit", "replit.nix"]

[nix]
channel = "stable-21_11"
